#!/bin/sh
# postrm script for openoffice.org-bin

THIS_PACKGAGE=openoffice.org-bin
THIS_SCRIPT=postrm

#INCLUDE_SHELL_LIB#

set -e

if [ "$1" = "purge" ]; then
	rm -f /etc/openoffice/settings.debconf
	rmdir /etc/openoffice 2>/dev/null || true
	rm -f /var/state/openoffice/ooo_is_prelinked
	rmdir /var/state/openoffice 2>/dev/null || true
fi

exit 0
