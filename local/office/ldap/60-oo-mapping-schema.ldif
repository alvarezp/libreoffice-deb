dn: cn=schema
attributetypes: (1.3.6.1.4.1.9770.2.1.1.1 NAME 'oocfgState' DESC 'state contains information for merging, possible values are: replaced, modified, deleted' SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 SINGLE-VALUE)
attributetypes: (1.3.6.1.4.1.9770.2.1.1.2 NAME 'oocfgFinalized' DESC 'determines whether a entry and it's subentries are allowed to be changed or not' SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 SINGLE-VALUE)
attributetypes: (1.3.6.1.4.1.9770.2.1.1.3 NAME 'oocfgTemplate' DESC 'name of a template used for sets' SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 SINGLE-VALUE)
attributetypes: (1.3.6.1.4.1.9770.2.1.1.4 NAME 'oocfgTemplateDn' DESC 'DN to locate a template' SYNTAX 1.3.6.1.4.1.1466.115.121.1.12 SINGLE-VALUE)
attributetypes: (1.3.6.1.4.1.9770.2.1.1.5 NAME 'oocfgItemType' DESC 'Datatype of a item. possiple datatypes are: string, boolean, short, int, long, double, binary' SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 SINGLE-VALUE)
attributetypes: (1.3.6.1.4.1.9770.2.1.1.6 NAME 'oocfgItemList' DESC 'Determines if the listitem is treated as multi or single value' SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 SINGLE-VALUE)
attributetypes: (1.3.6.1.4.1.9770.2.1.1.7 NAME 'oocfgItemValue' DESC 'Contains the value of the item' SYNTAX 1.3.6.1.4.1.1466.115.121.1.15)		
objectclasses: (1.3.6.1.4.1.9770.2.2.1.1 NAME 'oocfgGroup' DESC 'Base class for all configuration entries'	SUP 'top' MUST (objectclass $ cn) MAY (oocfgState $ oocfgFinalized))
objectclasses: (1.3.6.1.4.1.9770.2.2.1.2 NAME 'oocfgSet' DESC 'class identifying set with containing elements of a certain template' SUP 'oocfgGroup' MUST ( oocfgTemplate $ oocfgTemplateDn ))
objectclasses: (1.3.6.1.4.1.9770.2.2.1.3 NAME 'oocfgItemSet' DESC 'Set which contains elements of type oocfgItem' SUP 'top' MUST ( objectclass $ cn ) MAY (oocfgState $ oocfgFinalized))
objectclasses: (1.3.6.1.4.1.9770.2.2.1.4 NAME 'oocfgItem' DESC 'A item is a generic value representation used for propertysets' SUP 'oocfgGroup' MAY (oocfgItemType $ oocfgItemList $ oocfgItemValue))


