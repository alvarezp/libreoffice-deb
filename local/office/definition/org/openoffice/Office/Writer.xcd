							<schema:named-value name="W4WFL_NO_PCTEXT4_HDFT_MAINATTR " value="4096">
								<schema:description>Ignore setting of W4WFL_ALL_HDFT_MAINATTR when importing PCText-4 documents.</schema:description>
							</schema:named-value>
							<schema:named-value name="W4WFL_NO_PCTEXT4_STP_DEC2    " value="8192">
								<schema:description>De-activate workaround of W4W-bug in PCText-4 filter (trying to 'guess' correct Header hight).</schema:description>
							</schema:named-value>
						</schema:value-names>
						<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
					</schema:type-info>
					<default:data>0</default:data>
				</schema:value>
			</schema:group>
			<schema:group cfg:name="WinWord">
				<schema:value cfg:name="WW1F" cfg:type="long" cfg:writable="false">
					<schema:documentation>
						<schema:description>Flags for controling the import of WinWord 1.0 documents.</schema:description>
					</schema:documentation>
					<schema:type-info>
						<schema:value-names>
							<schema:named-value name="Off" value="0">
								<schema:description>This key is not used.</schema:description>
							</schema:named-value>
							<schema:named-value name="TAG_FLD" value="1">
								<schema:description>Fields are tagged as plaintext and get the type "Set variable"</schema:description>
							</schema:named-value>
							<schema:named-value name="ALLOW_FLD_CR         " value="64">
								<schema:description>Carriage Returns (CR) in fields are allowed</schema:description>
							</schema:named-value>
							<schema:named-value name="DONT_HARD_PROTECT    " value="256">
								<schema:description>An area that includes the complete main text is protected instead of the whole document.</schema:description>
							</schema:named-value>
						</schema:value-names>
						<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
					</schema:type-info>
					<default:data>0</default:data>
				</schema:value>
				<schema:value cfg:name="WW" cfg:type="long" cfg:writable="false">
					<schema:documentation>
						<schema:description>Several flags for importing WinWord documents.</schema:description>
					</schema:documentation>
					<schema:type-info>
						<schema:value-names>
							<schema:named-value name="Off" value="0">
								<schema:description>This key is not used.</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_TEXT" value="1">
								<schema:description>No text is imported (useful for testing, if it is possible to load the file)</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_STYLES" value="2">
								<schema:description>No import of styles</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_ZSTYLES" value="4">
								<schema:description>No import of character styles</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_APO" value="8">
								<schema:description>No import of position frames</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_HDFT" value="16">
								<schema:description>No import of headers and footers</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_FTN" value="32">
								<schema:description>No import of footnotes and endnotes</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_FLD" value="64">
								<schema:description>No import of fields</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_GRAF" value="128">
								<schema:description>No import of bitmap graphics, valid for WW6, WW7, WW8...</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_DEFTABS" value="256">
								<schema:description>No setting of default tabs</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_LRUL" value="512">
								<schema:description>No orientation, paper size, columns und no spaces left/right (neither for page nor for paragraph settings)</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_DEFSTYLES" value="1024">
								<schema:description>No import of pre-defined styles</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_TABLE" value="2048">
								<schema:description>No import of tables</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_OUTLINE" value="4096">
								<schema:description>Outline information is not imported</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_NUMRULE" value="8192">
								<schema:description>No import of outlining and numbering, only valid for WW(6,7)</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_IMPLPASP" value="16384">
								<schema:description>No import of implicit paragraph spacing (top/bottom) and line spacing</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_GRAFLAYER" value="32768">
								<schema:description>No import of vector graphics, valid for WinWord(6,7)</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_COMPLEX" value="65536">
								<schema:description>No import of fast-saved files.</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_OLE" value="131072">
								<schema:description>No import of OLE objects</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_OLE_TO_GRAF" value="262144">
								<schema:description>Transfering OLE objects into graphics</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_VCCONTROLS" value="524288">
								<schema:description>No import of controls (e.g. input fields or check boxes)</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_STD_STY_DYA" value="1048576">
								<schema:description>Ignore bottom spacing for paragraph style "Standard". </schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_COLS" value="2097152">
								<schema:description>No import of multi-columns</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_NO_IMPORT" value="536870912">
								<schema:description>Disables the WW6/7 import completely</schema:description>
							</schema:named-value>
						</schema:value-names>
						<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
					</schema:type-info>
					<default:data>0</default:data>
				</schema:value>
				<schema:value cfg:name="WW8" cfg:type="long" cfg:writable="false">
					<schema:documentation>
						<schema:description>Several flags for importing WinWord 8 documents.</schema:description>
					</schema:documentation>
					<schema:type-info>
						<schema:value-names>
							<schema:named-value name="Off" value="0">
								<schema:description>This key is not used.</schema:description>
							</schema:named-value>
							<schema:named-value name="WW8FL_NO_FLY_FOR_TXBX" value="1">
								<schema:description>Prevents the conversion from Escher text boxes to Writer frames.</schema:description>
							</schema:named-value>
							<schema:named-value name="EQUAL_TAB_BORDERDISTS" value="2">
								<schema:description>All four border distances of a cell are equal.
        </schema:description>
							</schema:named-value>
						</schema:value-names>
						<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
					</schema:type-info>
					<default:data>0</default:data>
				</schema:value>
				<schema:value cfg:name="WWF" cfg:type="long" cfg:writable="false">
					<schema:documentation>
						<schema:description>Key WWF stands for "WinWord Field". These flags are controling the import of fields in WW6 documents.</schema:description>
					</schema:documentation>
					<schema:type-info>
						<schema:value-names>
							<schema:named-value name="Off" value="0">
								<schema:description>This key is not used.</schema:description>
							</schema:named-value>
							<schema:named-value name="TAG_FLD" value="1">
								<schema:description>Fields are tagged as plaintext and get the type "Set variable"</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFFL_TAGS_DO_ID" value="2">
								<schema:description>Tags include the Winword field number in their variable names.</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFFL_TAGS_VISIBLE" value="4">
								<schema:description>Tag fields are always visible (Debug)</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_BOOK_TO_VAR_REF" value="8">
								<schema:description>Conversion of bookmarks to field types "Set Variable" or "Set Reference"</schema:description>
							</schema:named-value>
							<schema:named-value name="WWFL_BOOK_AND_REF " value="16">
								<schema:description>Creates additionally a bookmark, if WWFL_BOOK_TO_VAR_REF is set.</schema:description>
							</schema:named-value>
							<schema:named-value name="TAGS_IN_TEXT" value="32">
								<schema:description>Tags are imported as text (not as "Set Variable" field)</schema:description>
							</schema:named-value>
							<schema:named-value name="ALLOW_FLD_CR         " value="64">
								<schema:description>Carriage Returns (CR) in fields are allowed</schema:description>
							</schema:named-value>
							<schema:named-value name="NO_FLD_CR" value="128">
								<schema:description>Carriage Returns (CR) in fields are not allowed (also valid, if TAG_FLD is set)</schema:description>
							</schema:named-value>
							<schema:named-value name="DONT_HARD_PROTECT    " value="256">
								<schema:description>An area that includes the complete main text is protected instead of the whole document.</schema:description>
							</schema:named-value>
						</schema:value-names>
						<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
					</schema:type-info>
					<default:data>0</default:data>
				</schema:value>
				<schema:value cfg:name="WWFA0" cfg:type="long" cfg:writable="false">
					<schema:documentation>
						<schema:description>"WinWord-field tag Always". A bit-field with 32 bits for enabling the tagging of fields for every field type. WWFA0 controls the field types 0 to 31.</schema:description>
					</schema:documentation>
					<default:data>0</default:data>
				</schema:value>
				<schema:value cfg:name="WWFA1" cfg:type="long" cfg:writable="false">
