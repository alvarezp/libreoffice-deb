<!DOCTYPE schema:component SYSTEM "../../../../schema/schema.description.dtd">
<schema:component cfg:name="Writer" cfg:package="org.openoffice.Office" xml:lang="en-US" xmlns:schema="http://openoffice.org/2000/registry/schema/description" xmlns:default="http://openoffice.org/2000/registry/schema/default" xmlns:cfg="http://openoffice.org/2000/registry/instance">
	<schema:import cfg:component="org.openoffice.Office.Common"/>
	<schema:templates>
		<schema:group cfg:name="AgendaType">
			<schema:value cfg:name="Author" cfg:type="string">
				<schema:documentation>
					<schema:title>Responsible</schema:title>
					<schema:description>Responsible person for the topic</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="Topic" cfg:type="string">
				<schema:documentation>
					<schema:title>Agenda topic</schema:title>
					<schema:description>Title for each of the agenda topics</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="Duration" cfg:type="int">
				<schema:documentation>
					<schema:title>Duration</schema:title>
					<schema:description>Scheduled duration of each topic</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="Minute"/>
				</schema:type-info>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="StyleType">
			<schema:value cfg:name="PreferredStyle" cfg:type="int">
				<schema:documentation>
					<schema:title>Style - Modern/Classic/Decorative</schema:title>
					<schema:description>Preferred style for the template</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:value-names>
						<schema:named-value name="Modern" value="1"/>
						<schema:named-value name="Classic" value="2"/>
						<schema:named-value name="Decorative" value="3"/>
					</schema:value-names>
					<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
				</schema:type-info>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="SizeType">
			<schema:value cfg:name="Width" cfg:type="int">
				<schema:type-info>
					<schema:unit name="1/100 mm"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="Height" cfg:type="int">
				<schema:type-info>
					<schema:unit name="1/100 mm"/>
				</schema:type-info>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="PositionType">
			<schema:value cfg:name="Left" cfg:type="int">
				<schema:documentation>
					<schema:description>Left distance of the object</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="1/100 mm"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="Right" cfg:type="int">
				<schema:documentation>
					<schema:description>Right distance of the object</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="1/100 mm"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="Top" cfg:type="int">
				<schema:documentation>
					<schema:description>Top distance of the object</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="1/100 mm"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="Bottom" cfg:type="int">
				<schema:documentation>
					<schema:description>Bottom distance of the object</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="1/100 mm"/>
				</schema:type-info>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="AlignmentType">
			<schema:value cfg:name="Position" cfg:type="int">
				<schema:type-info>
					<schema:value-names>
						<schema:named-value name="Zero" value="0"/>
						<schema:named-value name="Top Left" value="1"/>
						<schema:named-value name="Top Center" value="2"/>
						<schema:named-value name="Top Right" value="3"/>
						<schema:named-value name="Bottom Left" value="4"/>
						<schema:named-value name="Bottom Right" value="5"/>
						<schema:named-value name="Center" value="6"/>
						<schema:named-value name="Right Margin" value="7"/>
						<schema:named-value name="Left Margin" value="8"/>
					</schema:value-names>
					<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
				</schema:type-info>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="TextElementType">
			<schema:value cfg:name="Visible" cfg:type="boolean">
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Content" cfg:type="string" cfg:localized="true">
				<default:data xml:lang="en-US" xsi:null="true"/>
				<default:data xml:lang="de" xsi:null="true"/>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="IntElementType">
			<schema:value cfg:name="Visible" cfg:type="boolean"/>
			<schema:value cfg:name="Content" cfg:type="int"/>
		</schema:group>
		<schema:group cfg:name="FileType">
			<schema:value cfg:name="Name" cfg:type="string">
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Filter" cfg:type="string">
				<default:data xsi:null="true"/>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="PositionXYType">
			<schema:value cfg:name="X" cfg:type="int">
				<schema:type-info>
					<schema:unit name="1/100 mm"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="Y" cfg:type="int">
				<schema:type-info>
					<schema:unit name="1/100 mm"/>
				</schema:type-info>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="DataSetType">
			<schema:value cfg:name="DataSource" cfg:type="string">
				<schema:documentation>
					<schema:description>Specifies the datasource</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="Command" cfg:type="string">
				<schema:documentation>
					<schema:description>The name of the database table or query</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="CommandType" cfg:type="short">
				<schema:documentation>
					<schema:description>Specifies, if the data comes from a table or a query</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="ColumnsToText" cfg:type="string">
				<schema:documentation>
					<schema:description>Name of the columns that have been selected for inserting data into text or fields, plus inserted text and line breaks</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="ColumnsToTable" cfg:type="string">
				<schema:documentation>
					<schema:description>Name of the columns, that have been selected for inserting data into tables, plus inserted text and line breaks</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="ParaStyle" cfg:type="string">
				<schema:documentation>
					<schema:description>Specifies the paragraph style for text or fields</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="TableAutoFormat" cfg:type="string">
				<schema:documentation>
					<schema:description>Specifies the format (AutoFormat) to be used for a table.</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="IsTable" cfg:type="boolean">
				<schema:documentation>
					<schema:description>Specifies, if the data is inserted into tables</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="IsField" cfg:type="boolean">
				<schema:documentation>
					<schema:description>Specifies, if the data is inserted into fields</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="IsHeadlineOn" cfg:type="boolean">
				<schema:documentation>
					<schema:description>Specifies, if the table should have a headline</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="IsEmptyHeadline" cfg:type="boolean">
				<schema:documentation>
					<schema:description>Specifies, if the table has an empty headline or if the headline contains the database column names.</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:set cfg:name="ColumnSet" cfg:element-type="ColumnSetType"/>
		</schema:group>
		<schema:group cfg:name="ColumnSetType">
			<schema:value cfg:name="ColumnName" cfg:type="string">
				<schema:documentation>
					<schema:description>Specifies the column name</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="ColumnIndex" cfg:type="int">
				<schema:documentation>
					<schema:description>Specifies the column index</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="IsNumberFormat" cfg:type="boolean">
				<schema:documentation>
					<schema:description>Specifies, if the database field is assigned to a number format.</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="IsNumberFormatFromDataBase" cfg:type="boolean">
				<schema:documentation>
					<schema:description>Specifies, if the format of the database or a user-defined format is used.</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="NumberFormat" cfg:type="string">
				<schema:documentation>
					<schema:description>Specifies the number format, if a user-defined format is used.</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="NumberFormatLocale" cfg:type="string">
				<schema:documentation>
					<schema:description>Specifies the language of the user-defined number format.</schema:description>
				</schema:documentation>
			</schema:value>
		</schema:group>
	</schema:templates>
	<schema:schema cfg:localized="false">
		<schema:group cfg:name="Content">
			<schema:group cfg:name="Display">
				<schema:value cfg:name="GraphicObject" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Display -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Contents - [Section] Display -->
					<schema:documentation>
						<schema:title>Graphics and Objects</schema:title>
						<schema:description>Specifies, if grahics and objects should be
				  displayed</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
				<schema:value cfg:name="Table" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Display -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Contents - [Section] Display -->
					<schema:documentation>
						<schema:title>Tables</schema:title>
						<schema:description>Specifies, if tables should be
				  displayed</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
				<schema:value cfg:name="DrawingControl" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Display -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Contents - [Section] Display -->
					<schema:documentation>
						<schema:title>Drawings and controls</schema:title>
						<schema:description>Specifies, if drawings and controls should be
				  displayed</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
				<schema:value cfg:name="FieldCode" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Display -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Contents - [Section] Display -->
					<schema:documentation>
						<schema:title>Field codes</schema:title>
						<schema:description>Specifies, if field codes should be
				  displayed</schema:description>
					</schema:documentation>
					<default:data>false</default:data>
				</schema:value>
				<schema:value cfg:name="Note" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Display -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Contents - [Section] Display -->
					<schema:documentation>
						<schema:title>Notes</schema:title>
						<schema:description>Specifies, if notes should be
				  displayed</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
                <schema:value cfg:name="PreventTips" cfg:type="boolean">
                    <!-- Responsible: OS -->
                    <!-- UIHints: Not accessible via user interface -->
					<schema:documentation>
                        <schema:title>Prevent help tips</schema:title>
                        <schema:description>Enables the writer to prevent the display of help tips programmatically. </schema:description>
					</schema:documentation>
                    <default:data>false</default:data>
				</schema:value>
            </schema:group>
			<schema:group cfg:name="Highlighting">
				<schema:value cfg:name="IndexEntry" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Highlighting -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Contents - [Section] Highlighting -->
					<schema:documentation>
						<schema:title>Index entries</schema:title>
						<schema:description>Specifies, if index entries should be set apart
				  from normal text by using gray highlighting.</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
				<schema:value cfg:name="Index" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Highlighting -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Contents - [Section] Highlighting -->
					<schema:documentation>
						<schema:title>Indexes</schema:title>
						<schema:description>Specifies, if indexes should be set apart from
				  normal text by using gray highlighting.</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
				<schema:value cfg:name="Footnote" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Highlighting -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Contents - [Section] Highlighting -->
					<schema:documentation>
						<schema:title>Footnotes</schema:title>
						<schema:description>Specifies, if footnotes should be set apart
				  from normal text by using gray highlighting.</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
				<schema:value cfg:name="Field" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Highlighting -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Contents - [Section] Highlighting -->
					<schema:documentation>
						<schema:title>Fields</schema:title>
						<schema:description>Specifies, if fields should be set apart from
				  normal text by using gray highlighting.</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
			</schema:group>
			<schema:group cfg:name="NonprintingCharacter">
                <schema:value cfg:name="MetaCharacters" cfg:type="boolean">
					<!-- Responsible: JP -->
                    <!-- UIHints: View - Nonprinting Characters -->
					<schema:documentation>
                        <schema:title>Meta characters</schema:title>
                        <schema:description>Determins whether certain settings are applied or not
                        (display of tab stops, spaces, breaks) </schema:description>
					</schema:documentation>
					<default:data>false</default:data>
				</schema:value>
                <schema:value cfg:name="ParagraphEnd" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Nonprinting_characters -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document  Contents - [Section] Nonprinting Characters -->
					<schema:documentation>
						<schema:title>Paragraph end</schema:title>
						<schema:description>Specifies, if paragraph ends are shown on the
				  screen</schema:description>
					</schema:documentation>
					<default:data>false</default:data>
				</schema:value>
				<schema:value cfg:name="OptionalHyphen" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Nonprinting_characters -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document  Contents - [Section] Nonprinting Characters -->
					<schema:documentation>
						<schema:title>Optional hyphens</schema:title>
						<schema:description>Specifies, if optional hyphens are shown on the
				  screen</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
				<schema:value cfg:name="Space" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Nonprinting_characters -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document  Contents - [Section] Nonprinting Characters -->
					<schema:documentation>
						<schema:title>Spaces</schema:title>
						<schema:description>Specifies, if spaces are shown on the
				  screen</schema:description>
					</schema:documentation>
					<default:data>false</default:data>
				</schema:value>
				<schema:value cfg:name="ProtectedSpace" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Nonprinting_characters -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document  Contents - [Section] Nonprinting Characters -->
					<schema:documentation>
						<schema:title>Protected Spaces</schema:title>
						<schema:description>Specifies, if protected spaces are shown on the
				  screen</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
				<schema:value cfg:name="Tab" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Nonprinting_characters -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document  Contents - [Section] Nonprinting Characters -->
					<schema:documentation>
						<schema:title>Tabs</schema:title>
						<schema:description>Specifies, if breaks are shown on the
				  screen</schema:description>
					</schema:documentation>
					<default:data>false</default:data>
				</schema:value>
				<schema:value cfg:name="Break" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Nonprinting_characters -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document  Contents - [Section] Nonprinting Characters -->
					<schema:documentation>
						<schema:title>Breaks</schema:title>
						<schema:description>Specifies, if paragraph ends are shown on the
				  screen</schema:description>
					</schema:documentation>
					<default:data>false</default:data>
				</schema:value>
				<schema:value cfg:name="HiddenText" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Nonprinting_characters -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document  Contents - [Section] Nonprinting Characters -->
					<schema:documentation>
						<schema:title>Hidden text</schema:title>
						<schema:description>Specifies, if hidden text should be displayed
				  on the screen</schema:description>
					</schema:documentation>
					<default:data>false</default:data>
				</schema:value>
				<schema:value cfg:name="HiddenParagraph" cfg:type="boolean">
					<!-- OldPath: Writer/Contents/Nonprinting_characters -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document  Contents - [Section] Nonprinting Characters -->
					<schema:documentation>
						<schema:title>Hidden paragraphs</schema:title>
						<schema:description>Specifies, if hidden paragraphs should be
				  displayed on the screen</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
			</schema:group>
			<schema:group cfg:name="Update">
				<schema:value cfg:name="Link" cfg:type="int">
					<!-- OldPath: Writer/Other/Update -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Other - [Section] Update links -->
					<schema:documentation>
						<schema:title>Update links: Always/On request/Never</schema:title>
						<schema:description>Specifies, if links in the document should be
				  updated during loading.</schema:description>
					</schema:documentation>
					<schema:type-info>
						<schema:value-names>
							<schema:named-value name="Always" value="0"/>
							<schema:named-value name="On Request" value="1"/>
							<schema:named-value name="Never" value="2"/>
						</schema:value-names>
						<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
					</schema:type-info>
					<default:data>1</default:data>
				</schema:value>
                <schema:value cfg:name="Field" cfg:type="boolean">
					<!-- OldPath: Writer/Other/Update -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Other - [Section] Fields and charts -->
					<schema:documentation>
						<schema:title>Update fields automatically</schema:title>
						<schema:description>Specifies, if fields in the document should be
				  updated during loading.</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
				<schema:value cfg:name="Chart" cfg:type="boolean">
					<!-- OldPath: Writer/Other/Update -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Other - [Section] Fields and charts -->
					<schema:documentation>
						<schema:title>Update charts automatically</schema:title>
						<schema:description>Specifies, if charts in the document should be
				  updated during loading.</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
				</schema:value>
            </schema:group>
		</schema:group>
		<schema:group cfg:name="Layout">
			<schema:group cfg:name="Line">
				<schema:value cfg:name="TextBoundary" cfg:type="boolean">
					<!-- OldPath: Writer/Layout/Lines -->
					<!-- OldLocation: Soffice.cfg -->
					<!-- Responsible: JP -->
					<!-- UIHints: Tools - Options - Text document - Layout - [Section] Lines -->
					<schema:documentation>
						<schema:title>Text boundaries</schema:title>
						<schema:description>Option to display the page margins as thin
				  lines</schema:description>
					</schema:documentation>
					<default:data>true</default:data>
