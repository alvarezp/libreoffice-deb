<!DOCTYPE schema:component SYSTEM "../../../../schema/schema.description.dtd">
<schema:component cfg:name="Common" cfg:package="org.openoffice.Office" xml:lang="en-US" xmlns:schema="http://openoffice.org/2000/registry/schema/description" xmlns:default="http://openoffice.org/2000/registry/schema/default" xmlns:cfg="http://openoffice.org/2000/registry/instance">
	<schema:templates>
		<schema:group cfg:name="Passwordstorage">
			<schema:value cfg:name="Password" cfg:type="string" cfg:localized="false" cfg:writable="true">
				<schema:documentation>
					<schema:description/>
				</schema:documentation>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="HistoryType">
			<schema:value cfg:name="URL" cfg:type="string">
				<schema:documentation>
					<schema:description>URL</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="Filter" cfg:type="string">
				<schema:documentation>
					<schema:description>Filtername</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="Title" cfg:type="string">
				<schema:documentation>
					<schema:description>Title of the URL which is displayed at the user-interface</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="Password" cfg:type="string">
				<schema:documentation>
					<schema:description>Encoded password</schema:description>
				</schema:documentation>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="MenuType">
			<schema:value cfg:name="URL" cfg:type="string">
				<schema:documentation>
					<schema:description>URL for dispatch</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="Title" cfg:type="string" cfg:localized="true">
				<schema:documentation>
					<schema:description>Label of menu entry</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="ImageIdentifier" cfg:type="string">
				<schema:documentation>
					<schema:description>Identifier of assigned icon</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="TargetName" cfg:type="string">
				<schema:documentation>
					<schema:description>target of dispatch (e.g. _blank to open URL in new frame)</schema:description>
				</schema:documentation>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="DialogSize">
			<schema:value cfg:name="Height" cfg:type="int">
				<schema:documentation>
					<schema:description>Height of the dialog</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="pixel"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="Width" cfg:type="int">
				<schema:documentation>
					<schema:description>Width of the dialog</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="pixel"/>
				</schema:type-info>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="AppWindowPosition">
			<schema:value cfg:name="Left" cfg:type="int">
				<schema:documentation>
					<schema:description>The postion of the application window from the left</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="pixel"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="Top" cfg:type="int">
				<schema:documentation>
					<schema:description>The position of the application window from the top</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="pixel"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="Right" cfg:type="int">
				<schema:documentation>
					<schema:description>The position of the application window from the right</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="pixel"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="Bottom" cfg:type="int">
				<schema:documentation>
					<schema:description>The position of the application window from the bottom</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="pixel"/>
				</schema:type-info>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="Font">
			<schema:documentation>
				<schema:description>describes the characteristics of a font.</schema:description>
			</schema:documentation>
			<schema:value cfg:name="Name" cfg:type="string" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies the exact name of the font ("Arial","Courier", "Frutiger").</schema:description>
				</schema:documentation>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Height" cfg:type="short" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies the height of the font in the measure of the destination.</schema:description>
				</schema:documentation>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Width" cfg:type="short" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies the width of the font in the measure of the destination.</schema:description>
				</schema:documentation>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="StyleName" cfg:type="string" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies the style name of the font ("Bold", "Italic", "Italic Bold").</schema:description>
				</schema:documentation>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Family" cfg:type="short" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies the general style of the font.</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:value-names>
						<schema:named-value value="0" name="DONTKNOW"/>
						<schema:named-value value="1" name="DECORATIVE"/>
						<schema:named-value value="2" name="MODERN"/>
						<schema:named-value value="3" name="ROMAN"/>
						<schema:named-value value="4" name="SCRIPT"/>
						<schema:named-value value="5" name="SWISS"/>
						<schema:named-value value="6" name="SYSTEM"/>
						<schema:named-value value="7" name="DONTKNOW"/>
					</schema:value-names>
				</schema:type-info>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="CharSet" cfg:type="short" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies the character set which is supported by the font.</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:value-names>
						<schema:named-value value="0" name="DONTKNOW"/>
						<schema:named-value value="1" name="ANSI"/>
						<schema:named-value value="2" name="MAC"/>
						<schema:named-value value="3" name="IBMPC_437"/>
						<schema:named-value value="4" name="IBMPC_850"/>
						<schema:named-value value="5" name="IBMPC_860"/>
						<schema:named-value value="6" name="IBMPC_861"/>
						<schema:named-value value="7" name="IBMPC_863"/>
						<schema:named-value value="8" name="IBMPC_865"/>
						<schema:named-value value="9" name="SYSTEM"/>
						<schema:named-value value="10" name="SYMBOL"/>
					</schema:value-names>
				</schema:type-info>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Pitch" cfg:type="short" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies the pitch of the font.</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:value-names>
						<schema:named-value value="0" name="DONTKNOW"/>
						<schema:named-value value="1" name="FIXED"/>
						<schema:named-value value="2" name="VARIABLE"/>
					</schema:value-names>
				</schema:type-info>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="CharacterWidth" cfg:type="double" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies the character width. Depending on the specified width, a font that supports this width may be selected.</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="%"/>
				</schema:type-info>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Weight" cfg:type="double" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies the thickness of the line. </schema:description>
				</schema:documentation>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Slant" cfg:type="short" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies if there is a character slant (italic).</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:value-names>
						<schema:named-value value="0" name="NONE"/>
						<schema:named-value value="1" name="OBLIQUE"/>
						<schema:named-value value="2" name="	ITALIC"/>
						<schema:named-value value="3" name="DONTKNOW"/>
						<schema:named-value value="4" name="REVERSE_OBLIQUE"/>
						<schema:named-value value="5" name="REVERSE_ITALIC"/>
					</schema:value-names>
				</schema:type-info>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="UnderLine" cfg:type="short" cfg:nullable="true">
				<schema:documentation>
					<schema:description/>
				</schema:documentation>
				<schema:type-info>
					<schema:value-names>
						<schema:named-value value="0" name="NONE"/>
						<schema:named-value value="1" name="SINGLE"/>
						<schema:named-value value="2" name="DOUBLE"/>
						<schema:named-value value="3" name="DOTTED"/>
						<schema:named-value value="4" name="DONTKNOW"/>
						<schema:named-value value="5" name="DASH"/>
						<schema:named-value value="6" name="LONGDASH"/>
						<schema:named-value value="7" name="DASHDOT"/>
						<schema:named-value value="8" name="DASHDOTDOT"/>
						<schema:named-value value="9" name="SMALLWAVE"/>
						<schema:named-value value="10" name="WAVE"/>
						<schema:named-value value="11" name="DOUBLEWAVE"/>
						<schema:named-value value="12" name="BOLD"/>
						<schema:named-value value="13" name="BOLDDOTTED"/>
						<schema:named-value value="14" name="BOLDDASH"/>
						<schema:named-value value="15" name="BOLDLONGDASH"/>
						<schema:named-value value="16" name="BOLDDASHDOT"/>
						<schema:named-value value="17" name="BOLDDASHDOTDOT"/>
						<schema:named-value value="18" name="BOLDWAVE"/>
					</schema:value-names>
				</schema:type-info>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Strikeout" cfg:type="short" cfg:nullable="true">
				<schema:documentation>
					<schema:description/>
				</schema:documentation>
				<schema:type-info>
					<schema:value-names>
						<schema:named-value value="0" name="NONE"/>
						<schema:named-value value="1" name="SINGLE"/>
						<schema:named-value value="2" name="DOUBLE"/>
						<schema:named-value value="3" name="DONTKNOW"/>
						<schema:named-value value="4" name="BOLD"/>
						<schema:named-value value="5" name="SLASH"/>
						<schema:named-value value="6" name="X"/>
					</schema:value-names>
				</schema:type-info>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Orientation" cfg:type="double" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies the rotation of the font.</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="degree"/>
				</schema:type-info>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Kerning" cfg:type="boolean" cfg:nullable="true">
				<schema:documentation>
					<schema:description>for requesting, it specifies if there is a kerning table available. For selecting, it specifies if the kerning table is to be used.</schema:description>
				</schema:documentation>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="WordLineMode" cfg:type="boolean" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies if only words get underlined.</schema:description>
				</schema:documentation>
				<default:data xsi:null="true"/>
			</schema:value>
			<schema:value cfg:name="Type" cfg:type="short" cfg:nullable="true">
				<schema:documentation>
					<schema:description>specifies the technology of the font representation.</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:value-names>
						<schema:named-value value="0" name="DONTKNOW"/>
						<schema:named-value value="1" name="RASTER"/>
						<schema:named-value value="2" name="DEVICE"/>
						<schema:named-value value="3" name="SCALABLE"/>
					</schema:value-names>
				</schema:type-info>
				<default:data xsi:null="true"/>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="FontReplacement">
			<schema:value cfg:name="ReplaceFont" cfg:type="string">
				<schema:documentation>
					<schema:description>The name of font that is to be replaced</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="SubstituteFont" cfg:type="string">
				<schema:documentation>
					<schema:description>The name of the replacing font</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="OnScreenOnly" cfg:type="boolean">
				<schema:documentation>
					<schema:description>Determines if the font pair replacement is applied on the screen</schema:description>
				</schema:documentation>
				<default:data>false</default:data>
			</schema:value>
			<schema:value cfg:name="Always" cfg:type="boolean">
				<schema:documentation>
					<schema:description>Determines if the font pair replacement is applied on the printer</schema:description>
				</schema:documentation>
				<default:data>false</default:data>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="GraphicSize">
			<schema:value cfg:name="Width" cfg:type="int">
				<schema:documentation>
					<schema:description>Specifies the export width for the graphic format.</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="1/100 mm"/>
					<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema">
						<xsd:minInclusive value="100"/>
						<xsd:maxInclusive value="99999"/>
					</schema:constraints>
				</schema:type-info>
				<default:data>10000</default:data>
			</schema:value>
			<schema:value cfg:name="Height" cfg:type="int">
				<schema:documentation>
					<schema:description>Specifies the export height for the graphic format.</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="1/100 mm"/>
					<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema">
						<xsd:minInclusive value="100"/>
						<xsd:maxInclusive value="99999"/>
					</schema:constraints>
				</schema:type-info>
				<default:data>10000</default:data>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="WindowAligment">
			<schema:value cfg:name="Mode" cfg:type="int">
				<schema:documentation>
					<schema:description>The current alignment mode of the used window</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:value-names>
						<schema:named-value name="HighestTop" value="0"/>
						<schema:named-value name="LowestBottom" value="1"/>
						<schema:named-value name="FirstLeft" value="2"/>
						<schema:named-value name="LastRight" value="3"/>
						<schema:named-value name="Left" value="4"/>
						<schema:named-value name="Right" value="5"/>
						<schema:named-value name="LastLeft" value="6"/>
						<schema:named-value name="FirstRight" value="7"/>
						<schema:named-value name="Top" value="8"/>
						<schema:named-value name="Bottom" value="9"/>
						<schema:named-value name="ToolBoxTop" value="10"/>
						<schema:named-value name="ToolBoxBottom" value="11"/>
						<schema:named-value name="LowestTop" value="12"/>
						<schema:named-value name="HighestBottom" value="13"/>
						<schema:named-value name="ToolBoxLeft" value="14"/>
						<schema:named-value name="ToolBoxRight" value="15"/>
						<schema:named-value name="NoAligment" value="16"/>
					</schema:value-names>
					<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="LastMode" cfg:type="int">
				<schema:documentation>
					<schema:description>The last used alignment mode of the window</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:value-names>
						<schema:named-value name="HighestTop" value="0"/>
						<schema:named-value name="LowestBottom" value="1"/>
						<schema:named-value name="FirstLeft" value="2"/>
						<schema:named-value name="LastRight" value="3"/>
						<schema:named-value name="Left" value="4"/>
						<schema:named-value name="Right" value="5"/>
						<schema:named-value name="LastLeft" value="6"/>
						<schema:named-value name="FirstRight" value="7"/>
						<schema:named-value name="Top" value="8"/>
						<schema:named-value name="Bottom" value="9"/>
						<schema:named-value name="ToolBoxTop" value="10"/>
						<schema:named-value name="ToolBoxBottom" value="11"/>
						<schema:named-value name="LowestTop" value="12"/>
						<schema:named-value name="HighestBottom" value="13"/>
						<schema:named-value name="ToolBoxLeft" value="14"/>
						<schema:named-value name="ToolBoxRight" value="15"/>
						<schema:named-value name="NoAligment" value="16"/>
					</schema:value-names>
					<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="Line" cfg:type="int">
				<schema:documentation>
					<schema:description>Describes the position of the docked window.</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="Position" cfg:type="int"/>
			<schema:value cfg:name="Width" cfg:type="int">
				<schema:documentation>
					<schema:description>Width of the window</schema:description>
				</schema:documentation>
			</schema:value>
			<schema:value cfg:name="Height" cfg:type="int">
				<schema:documentation>
					<schema:description>Height of the window</schema:description>
				</schema:documentation>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="WindowSize">
			<schema:value cfg:name="Width" cfg:type="int">
				<schema:documentation>
					<schema:description>Width of the window</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="pixel"/>
				</schema:type-info>
			</schema:value>
			<schema:value cfg:name="Height" cfg:type="int">
				<schema:documentation>
					<schema:description>Height of the window</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="pixel"/>
				</schema:type-info>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="WindowPosition">
			<schema:value cfg:name="PositionX" cfg:type="int"/>
			<schema:value cfg:name="PositionY" cfg:type="int"/>
		</schema:group>
		<schema:group cfg:name="LayoutSettings">
			<schema:value cfg:name="MeasureUnit" cfg:type="int">
				<!-- OldPath: Writer/Layout -->
				<!-- OldLocation: Soffice.cfg -->
				<!-- Responsible: JP -->
				<!-- UIHints: Tools - Options - Text document - Layout - [Section] Meas. Units -->
				<schema:documentation>
					<schema:title>Measure unit</schema:title>
					<schema:description>Specifies the measurement
				  units</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:value-names>
						<schema:named-value name="Millimeter" value="1"/>
						<schema:named-value name="Centimeter" value="2"/>
						<schema:named-value name="Meter" value="3"/>
						<schema:named-value name="Kilometer" value="4"/>
						<schema:named-value name="Point" value="6"/>
						<schema:named-value name="Pica" value="7"/>
						<schema:named-value name="Inch" value="8"/>
						<schema:named-value name="Foot" value="9"/>
						<schema:named-value name="Miles" value="10"/>
					</schema:value-names>
					<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
				</schema:type-info>
				<default:data>2</default:data>
			</schema:value>
			<schema:value cfg:name="TabStop" cfg:type="int">
				<!-- OldPath: Writer/Layout -->
				<!-- OldLocation: Soffice.cfg -->
				<!-- Responsible: JP -->
				<!-- UIHints: Tools - Options - Text document - Layout - [Section] Tab stops -->
				<!-- Notice: The NonMetric default value : 1270-->
				<schema:documentation>
					<!-- xml:lang: true -->
					<schema:title>Tab stops</schema:title>
					<schema:description>Specifies the distance between tab
				  stops</schema:description>
				</schema:documentation>
				<schema:type-info>
					<schema:unit name="1/100 mm"/>
				</schema:type-info>
				<default:data>1250</default:data>
			</schema:value>
		</schema:group>
		<schema:group cfg:name="ObjectNames">
			<schema:documentation>
				<schema:description>Specifies the object names for the different Office modules. Thereby each module is assigned to a worldwide unique key. The different object names are filled by the setup.</schema:description>
			</schema:documentation>
			<schema:value cfg:name="Name" cfg:type="string" cfg:writable="false" cfg:localized="true"/>
			<schema:value cfg:name="Key" cfg:type="string" cfg:writable="false"/>
