                    </default:value>
                    <default:value schema:ref="ooInetPrefix">
                        <default:data>http://www.altavista.com/sites/search/web?q=</default:data>
                    </default:value>
                    <default:value schema:ref="ooInetSeparator">
                        <default:data>+</default:data>
                    </default:value>
                    <default:value schema:ref="ooInetSuffix">
                        <default:data>&amp;kl=XX&amp;pg=q</default:data>
                    </default:value>
                </default:group>
				<default:group cfg:name="Or" schema:ref="ooInetSearchParam">
