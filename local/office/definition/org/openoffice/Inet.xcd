<!DOCTYPE schema:component SYSTEM "../../../schema/schema.description2.dtd">
<schema:component cfg:name="Inet" cfg:package="org.openoffice" 
		schema:category="PublicApplProfile" xml:lang="en-US" xmlns:schema="http://openoffice.org/2000/registry/schema/description" xmlns:default="http://openoffice.org/2000/registry/schema/default" xmlns:cfg="http://openoffice.org/2000/registry/instance">
	<schema:types schema:component-id="1.1.2">
		<schema:value cfg:name="ooInetPrefix" cfg:type="string" schema:type-id="1">
			<schema:documentation>
				<schema:description>Specifies the search expression, i.e. URL and query command.</schema:description>
			</schema:documentation>
		</schema:value>
		<schema:value cfg:name="ooInetSuffix" cfg:type="string" schema:type-id="2">
			<schema:documentation>
				<schema:description>Specifies the suffix.</schema:description>
			</schema:documentation>
		</schema:value>
		<schema:value cfg:name="ooInetSeparator" cfg:type="string" schema:type-id="3">
			<schema:documentation>
				<schema:description>Specifies the separator</schema:description>
			</schema:documentation>
		</schema:value>
		<schema:value cfg:name="ooInetCaseMatch" cfg:type="int" schema:type-id="4">
			<schema:documentation>
				<schema:description>Specifies if upper and lower case spelling should be considered.</schema:description>
			</schema:documentation>
			<schema:type-info>
				<schema:value-names>
					<schema:named-value name="None" value="0"/>
					<schema:named-value name="Upper" value="1"/>
					<schema:named-value name="Lower" value="2"/>
				</schema:value-names>
				<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
			</schema:type-info>
		</schema:value>
		<schema:value cfg:name="ooInetDNSServer" cfg:type="string" schema:type-id="5">
			<!-- OldPath: Inet/DNS -->
			<!-- OldLocation: soffice.ini -->
			<!-- Responsible: SB/MHU -->
			<!-- UIHints: Tools - Options - Internet - Protocol -->
			<schema:documentation>
				<schema:description>Specifies the IP address of the DNS server. If the string is empty, the DNS will be specified automatically.</schema:description>
			</schema:documentation>
		</schema:value>		
		<schema:value cfg:name="ooInetNoProxy" cfg:type="string" schema:type-id="7">
			<!-- OldPath: Inet/NoProxy -->
			<!-- OldLocation: soffice.ini -->
			<!-- Responsible: SB/MHU -->
			<!-- UIHints: Tools  Options  Internet  Proxy -->
			<schema:documentation>
				<schema:description>Enumeration of the names of the servers that do not require any Proxy servers.</schema:description>
			</schema:documentation>
		</schema:value>
		<schema:value cfg:name="ooInetProxyType" cfg:type="int" schema:type-id="8">
			<!-- OldPath: Inet/ProxyType -->
			<!-- OldLocation: soffice.ini -->
			<!-- Responsible: SB/MHU -->
			<!-- UIHints: Tools  Options  Internet  Proxy -->
			<schema:documentation>
				<schema:description>Specifies the type of the Proxy selection.</schema:description>
			</schema:documentation>
			<schema:type-info>
				<schema:value-names>
					<schema:named-value name="None" value="0"/>
					<schema:named-value name="Automatic" value="1"/>
					<schema:named-value name="Manual" value="2"/>
				</schema:value-names>
				<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema"/>
			</schema:type-info>			
		</schema:value>			
		<schema:value cfg:name="ooInetFTPProxyName" cfg:type="string" schema:type-id="9">
			<!-- OldPath: Inet/FTPProxyName -->
			<!-- OldLocation: soffice.ini -->
			<!-- Responsible: SB/MHU -->
			<!-- UIHints: Tools  Options  Internet  Proxy -->
			<schema:documentation>
				<schema:description>Name of the FTP Proxy server</schema:description>
			</schema:documentation>
		</schema:value>
		<schema:value cfg:name="ooInetFTPProxyPort" cfg:type="int" schema:type-id="10">
			<!-- OldPath: Inet/FTPProxyPort -->
			<!-- OldLocation: soffice.ini -->
			<!-- Responsible: SB/MHU -->
			<!-- UIHints: Tools  Options  Internet  Proxy -->
			<schema:documentation>
				<schema:description>Port of the FTP Proxy server</schema:description>
			</schema:documentation>
			<schema:type-info>
				<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema">
					<xsd:minInclusive value="0"/>
					<xsd:maxInclusive value="65535"/>
				</schema:constraints>
			</schema:type-info>
		</schema:value>			
		<schema:value cfg:name="ooInetHTTPProxyName" cfg:type="string" schema:type-id="11">
			<!-- OldPath: Inet/HTTPProxyName -->
			<!-- OldLocation: soffice.ini -->
			<!-- Responsible: SB/MHU -->
			<!-- UIHints: Tools  Options  Internet  Proxy -->
			<schema:documentation>
				<schema:description>Name of the HTTP Proxy server</schema:description>
			</schema:documentation>
		</schema:value>
		<schema:value cfg:name="ooInetHTTPProxyPort" cfg:type="int" schema:type-id="12">
			<!-- OldPath: Inet/HTTPProxyPort -->
			<!-- OldLocation: soffice.ini -->
			<!-- Responsible: SB/MHU -->
			<!-- UIHints: Tools  Options  Internet  Proxy -->
			<schema:documentation>
				<schema:description>Port of the HTTP Proxy server</schema:description>
			</schema:documentation>
			<schema:type-info>
				<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema">
					<xsd:minInclusive value="0"/>
					<xsd:maxInclusive value="65535"/>
				</schema:constraints>
			</schema:type-info>
		</schema:value>
		<schema:value cfg:name="ooInetSOCKSProxyName" cfg:type="string" schema:type-id="13">
			<!-- OldPath: Inet/SOCKSProxyName -->
			<!-- OldLocation: soffice.ini -->
			<!-- Responsible: SB/MHU -->
			<!-- UIHints: Tools  Options  Internet  Proxy -->
			<schema:documentation>
				<schema:description>Name of the Socks Proxy server</schema:description>
			</schema:documentation>
		</schema:value>
		<schema:value cfg:name="ooInetSOCKSProxyPort" cfg:type="int" schema:type-id="14">
			<!-- OldPath: Inet/SOCKSProxyPort -->
			<!-- OldLocation: soffice.ini -->
			<!-- Responsible: SB/MHU -->
			<!-- UIHints: Tools  Options  Internet  Proxy -->
			<schema:documentation>
				<schema:description>Port of the Socks Proxy server</schema:description>
			</schema:documentation>
			<schema:type-info>
				<schema:constraints xmlns:xsd="http://www.w3.org/1999/XMLSchema">
					<xsd:minInclusive value="0"/>
					<xsd:maxInclusive value="65535"/>
				</schema:constraints>
			</schema:type-info>
		</schema:value>			
		<schema:class cfg:name="ooInetSearchParam" schema:type-id="1">
			<schema:documentation>
				<schema:description>Specifies the search settings for a search machine.</schema:description>
			</schema:documentation>
			<schema:member schema:ref="ooInetPrefix"/>
			<schema:member schema:ref="ooInetSuffix"/>
			<schema:member schema:ref="ooInetSeparator"/>
			<schema:member schema:ref="ooInetCaseMatch"/>
		</schema:class>		
		<schema:class cfg:name="ooInetSettings" schema:type-id="2">
			<schema:member schema:ref="ooInetDNSServer"/>
			<schema:member schema:ref="ooInetNoProxy"/>
			<schema:member schema:ref="ooInetProxyType"/>			
			<schema:member schema:ref="ooInetFTPProxyName"/>
			<schema:member schema:ref="ooInetFTPProxyPort"/>
			<schema:member schema:ref="ooInetHTTPProxyName"/>
			<schema:member schema:ref="ooInetHTTPProxyPort"/>
			<schema:member schema:ref="ooInetSOCKSProxyName"/>
			<schema:member schema:ref="ooInetSOCKSProxyPort"/>
		</schema:class>
	</schema:types>	
	<schema:templates>		
		<schema:group cfg:name="SearchEngine">
			<schema:documentation>
				<schema:description>Specifies the search type, i.e. nexus or exact search.</schema:description>
			</schema:documentation>
			<schema:group cfg:name="And" schema:ref="ooInetSearchParam"/>
			<schema:group cfg:name="Or"	 schema:ref="ooInetSearchParam"/>
			<schema:group cfg:name="Exact" schema:ref="ooInetSearchParam"/>
		</schema:group>		
	</schema:templates>
	<schema:schema>
