                    </default:value>
                    <default:value schema:ref="ooInetSeparator">
                        <default:data>+OR+</default:data>
                    </default:value>
                    <default:value schema:ref="ooInetSuffix">
                        <default:data>&amp;kl=XX&amp;search=Search</default:data>
                    </default:value>
                </default:group>
				<default:group cfg:name="Exact" schema:ref="ooInetSearchParam">
                    <default:value schema:ref="ooInetPrefix">
                        <default:data>http://www.altavista.com/sites/search/web?q=%22</default:data>
                    </default:value>
                    <default:value schema:ref="ooInetSeparator">
                        <default:data>+</default:data>
                    </default:value>
                    <default:value schema:ref="ooInetSuffix">
                        <default:data>%22&amp;kl=XX&amp;search=Search</default:data>
                    </default:value>
                    <default:value schema:ref="ooInetCaseMatch">
                        <default:data>0</default:data>
                    </default:value>
                </default:group>
			</default:group>
