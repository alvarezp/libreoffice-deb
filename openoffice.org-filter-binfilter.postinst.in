#!/bin/sh

set -e

THIS_PACKAGE=openoffice.org-filter-binfilter
THIS_SCRIPT=postinst

#INCLUDE_SHELL_LIB#

if [ "$1" = "configure" -o "$1" = "upgrade" ]; then
	db_input high openoffice.org-filter-binfilter/so5x-filters-going-away || true
	db_go || true
	register_to_services_rdb /@OOBASISDIR@/program/libbf_migratefilter$LIBSUFFIX.so
fi

#DEBHELPER#

exit 0
