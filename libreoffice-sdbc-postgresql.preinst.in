#!/bin/sh

set -e

#INCLUDE_SHELL_LIB#

case "$1" in
	upgrade)
		case "`echo $2 | cut -d+ -f2`" in)
		OOo*)
			# this is a legacy package, so we need to find it first...
			id=`/usr/lib/libreoffice/program/unopkg list --shared | grep Identifier:.*postgresql-sdbc | cut -d":" -f2`
			remove_extension $id
			;;
		esac
	;;
esac

#DEBHELPER#

exit 0
