#!/bin/sh

set -e

#INCLUDE_SHELL_LIB#

case "$1" in
	install|upgrade)
		if test -n "$2" && dpkg --compare-versions "$2" lt "2.0.0-1"; then
			prep_rm_conffile "/etc/openoffice/sofficerc" $DPKG_MAINTSCRIPT_PACKAGE
			prep_rm_conffile "/etc/openoffice/psprint.conf" $DPKG_MAINTSCRIPT_PACKAGE
		fi
	;;
esac

#DEBHELPER#

exit 0
