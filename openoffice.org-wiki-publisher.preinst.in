#!/bin/sh

set -e

# preinst script for openoffice.org-wiki-publisher

THIS_PACKAGE=openoffice.org-wiki-publisher
THIS_SCRIPT=preinst

#INCLUDE_SHELL_LIB#

case "$1" in
	upgrade)
		if dpkg --compare-versions $1 lt 1:3.0.0~dev300m24; then
			remove_extension com.sun.wiki-publisher
		else
			remove_extension com.sun.star.WikiPublisher
		fi
	;;
esac

#DEBHELPER#

exit 0
