#!/bin/sh
#preinst for openoffice.org-gnomevfs

THIS_PACKAGE=openoffice.org-gnomevfs
THIS_SCRIPT=preinst

#INCLUDE_SHELL_LIB#

set -e

case "$1" in
    install|upgrade)
        for file in $gnomevfs_diversions; do
            dpkg-divert --add --rename --package $THIS_PACKAGE \
              --divert /$file.nognomevfs /$file
        done
    ;;

    abort-upgrade)
    ;;

    *)
        echo "preinst called with unknown argument \'$1'" >&2
	exit 1
    ;;
esac

exit 0
