#!/bin/sh

set -e

#INCLUDE_SHELL_LIB#

case "$1" in
	remove)
		# this is a legacy package, so we need to find it first...
                id=`/usr/lib/openoffice/program/unopkg list --shared | grep Identifier:.*postgresql-sdbc | cut -d":" -f2`
                remove_extension $id
	;;
esac

#DEBHELPER#

exit 0
