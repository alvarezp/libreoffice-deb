#!/bin/sh
# prerm script for openoffice.org

THIS_PACKGAGE=openoffice.org
THIS_SCRIPT=prerm

#INCLUDE_SHELL_LIB#

set -e

if [ "$1" = "remove" ]; then
	if [ -L /usr/lib/openoffice/share/dict/ooo ]; then
		rm /usr/lib/openoffice/share/dict/ooo
	fi
	rmdir /usr/lib/openoffice/share/dict || true
fi

exit 0
