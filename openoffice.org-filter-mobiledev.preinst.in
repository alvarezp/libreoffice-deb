#!/bin/sh

set -e

THIS_PACKAGE=openoffice.org-filter-mobiledev
THIS_SCRIPT=preinst

#INCLUDE_SHELL_LIB#

case "$1" in install|upgrade)
	if [ -L /usr/lib/openoffice/program/classes ]; then
		rm /usr/lib/openoffice/program/classes
		mkdir /usr/lib/openoffice/program/classes
	fi
	;;
esac

#DEBHELPER#

exit 0
