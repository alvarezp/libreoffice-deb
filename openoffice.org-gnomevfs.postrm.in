#!/bin/sh
# postrm script for openoffice.org-gnomevfs

THIS_PACKAGE=openoffice.org-gnomevfs
THIS_SCRIPT=postrm

#INCLUDE_SHELL_LIB#

set -e

case "$1" in
    remove)
        for file in $gnomevfs_diversions; do
            dpkg-divert --remove --rename --package $THIS_PACKAGE \
              --divert /$file.nognomevfs /$file
        done
    ;;

    purge)
    ;;

    upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
    ;;

    *)
        echo "postrm called with unknown argument \'$1'" >&2
	exit 1
    ;;
esac

exit 0
