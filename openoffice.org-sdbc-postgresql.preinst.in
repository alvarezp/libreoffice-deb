#!/bin/sh

set -e

#INCLUDE_SHELL_LIB#

case "$1" in
	upgrade)
		if dpkg --compare-versions `echo $2 | cut -d+ -f2` lt OOo3.3.0~beta~dev300m84; then
			# this is a legacy package, so we need to find it first...
			id=`/usr/lib/openoffice/program/unopkg list --shared | grep Identifier:.*postgresql-sdbc | cut -d":" -f2`
			remove_extension $id
		fi
	;;
esac

#DEBHELPER#

exit 0
