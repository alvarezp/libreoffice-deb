#!/bin/sh

set -e

THIS_PACKAGE=openoffice.org-sdbc-postgresql
THIS_SCRIPT=preinst

#INCLUDE_SHELL_LIB#

case "$1" in
	upgrade)
		# this is a legacy package, so we need to find it first...
		id=`/usr/lib/openoffice/program/unopkg list --shared | grep Identifier:.*postgresql-sdbc | cut -d":" -f2`
		remove_extension $id
	;;
esac

#DEBHELPER#

exit 0
