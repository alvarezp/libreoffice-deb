# This file is NOT a shell script.
#
# This file gets included by both debian/rules (make) AND the scripts in
# debian/scripts (Bourne shell).
ENVFILE = LinuxPPCEnv.Set
ARCHBUILDDIR=unxlngppc.pro
JDK_HOME="/usr/lib/j2se/1.3"

#depends
DEPS="libstdc++4, libc6 (>=2.2.5-9.1), libstlport4.5gcc3.1 (>=4.5.3-5cjh2)"

#Use GCC 3.0.x?
USE_GCC3=y
#Set the gcc-version. (Only for gcc-3.0 and gcc-3.1)
GCC3VER=3.1
